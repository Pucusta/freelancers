				"          ef_money_sd as string,",
				"          ef_money_sd_data as string,",
				"          ef_money_inflation as string,",
				"          ef_money_inflation_data as string,",
				"          ef_money_currency as string,",
				"          ef_money as string,",
				"          ef_trade_tariffs_revenue as string,",
				"          ef_trade_tariffs_revenue_data as string,",
				"          ef_trade_tariffs_mean as string,",
				"          ef_trade_tariffs_mean_data as string,",
				"          ef_trade_tariffs_sd as string,",
				"          ef_trade_tariffs_sd_data as string,",
				"          ef_trade_tariffs as string,",
				"          ef_trade_regulatory_nontariff as string,",
				"          ef_trade_regulatory_compliance as string,",
				"          ef_trade_regulatory as string,",
				"          ef_trade_black as string,",
				"          ef_trade_movement_open as string,",
				"          ef_trade_movement_capital as string,",
				"          ef_trade_movement_visit as string,",
				"          ef_trade_movement as string,",
				"          ef_trade as string,",
				"          ef_regulation_credit_ownership as string,",
				"          ef_regulation_credit_private as string,",
				"          ef_regulation_credit_interest as string,",
				"          ef_regulation_credit as string,",
				"          ef_regulation_labor_minwage as string,",
				"          ef_regulation_labor_firing as string,",
				"          ef_regulation_labor_bargain as string,",
				"          ef_regulation_labor_hours as string,",
				"          ef_regulation_labor_dismissal as string,",
				"          ef_regulation_labor_conscription as string,",
				"          ef_regulation_labor as string,",
				"          ef_regulation_business_adm as string,",
				"          ef_regulation_business_burden as string,",
				"          ef_regulation_business_start as string,",
				"          ef_regulation_business_impartial as string,",
				"          ef_regulation_business_licensing as string,",
				"          ef_regulation_business_compliance as string,",
				"          ef_regulation_business as string,",
				"          ef_regulation as string,",
				"          ef_score as string,",
				"          ef_rank as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> hfi",
				"source(output(",
				"          Country as string,",
				"          Year as string,",
				"          Status as string,",
				"          {Life expectancy } as string,",
				"          {Adult Mortality} as string,",
				"          {infant deaths} as string,",
				"          Alcohol as string,",
				"          {percentage expenditure} as string,",
				"          {Hepatitis B} as string,",
				"          {Measles } as string,",
				"          { BMI } as string,",
				"          {under-five deaths } as string,",
				"          Polio as string,",
				"          {Total expenditure} as string,",
				"          {Diphtheria } as string,",
				"          { HIV/AIDS} as string,",
				"          GDP as string,",
				"          Population as string,",
				"          { thinness  1-19 years} as string,",
				"          { thinness 5-9 years} as string,",
				"          {Income composition of resources} as string,",
				"          Schooling as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> lifeexpectancy",
				"hfi filter(year == '2017') ~> filter1",
				"lifeexpectancy filter(Year == '2015') ~> filter2",
				"trim1 derive(Country = case(\r",
				"    Country == 'Antigua & Barbuda', 'Antigua and Barbuda',\r",
				"    Country == 'Bahamas, The', 'Bahamas',\r",
				"    Country == 'Bosnia & Herzegovina', 'Bosnia and Herzegovina',\r",
				"    Country == 'Central African Rep.', 'Central African Republic',\r",
				"    Country == 'Congo, Dem. Rep.', 'Democratic Republic of the Congo',\r",
				"    Country == 'Congo, Repub. of the', 'Republic of the Congo',\r",
				"    Country == 'Gambia, The', 'Gambia',\r",
				"    Country == 'Korea, South', 'South Korea',\r",
				"    Country == 'Saint Kitts & Nevis', 'Saint Kitts and Nevis',\r",
				"    Country == 'Sao Tome & Principe', 'Sao Tome and Principe',\r",
				"    Country == 'Trinidad & Tobago', 'Trinidad and Tobago',\r",
				"    Country\r",
				")) ~> countryNameFix1",
				"countries derive(Country = trim(Country)) ~> trim1",
				"happiness derive(Country = trim(Country)) ~> trim2",
				"filter1 derive(countries = trim(countries)) ~> trim3",
				"filter2 derive(Country = trim(Country)) ~> trim4",
				"trim2 derive(Country = case(\r",
				"    Country == 'Congo (Brazzaville)', 'Republic of the Congo',\r",
				"    Country == 'Congo (Kinshasa)', 'Democratic Republic of the Congo',\r",
				"    Country == 'Hong Kong S.A.R., China', 'Hong Kong',\r",
				"    Country == 'Taiwan Province of China', 'Taiwan',\r",
				"    Country\r",
				")) ~> countryNameFix2",
				"trim3 derive(countries = case(\r",
				"    countries == 'Brunei Darussalam', 'Brunei',\r",
				"    countries == 'Congo, Dem. Rep.', 'Democratic Republic of the Congo',\r",
				"    countries == 'Congo, Rep.', 'Republic of the Congo',\r",
				"    countries == 'Egypt, Arab Rep.', 'Egypt',\r",
				"    countries == 'Hong Kong SAR, China', 'Hong Kong',\r",
				"    countries == 'Iran, Islamic Rep.', 'Iran',\r",
				"    countries == 'Korea, Rep.', 'South Korea',\r",
				"    countries == 'Kyrgyz Republic', 'Kyrgyzstan',\r",
				"    countries == 'Lao PDR', 'Laos',\r",
				"    countries == 'North Macedonia', 'Macedonia',\r",
				"    countries == 'Russian Federation', 'Russia',\r",
				"    countries == 'Slovak Republic', 'Slovakia',\r",
				"    countries == 'Syrian Arab Republic', 'Syria',\r",
				"    countries == 'Venezuela, RB', 'Venezuela',\r",
				"    countries == 'Yemen, Rep.', 'Yemen',\r",
				"    countries\r",
				")) ~> countryNameFix3",
				"trim4 derive(Country = case(\r",
				"    Country == 'Bolivia (Plurinational State of)', 'Bolivia',\r",
				"    Country == 'Brunei Darussalam', 'Brunei',\r",
				"    Country == 'Czechia', 'Czech Republic',\r",
				"    Country == 'Congo', 'Republic of the Congo',\r",
				"    Country == 'Iran (Islamic Republic of)', 'Iran',\r",
				"    Country == 'Republic of Korea', 'South Korea',\r",
				"    Country == 'Republic of Moldova', 'Moldova',\r",
				"    Country == 'Russian Federation', 'Russia',\r",
				"    Country == 'Syrian Arab Republic', 'Syria',\r",
				"    Country == 'The former Yugoslav republic of Macedonia', 'Macedonia',\r",
				"    Country == 'United Kingdom of Great Britain and Northern Ireland', 'United Kingdom',\r",
				"    Country == 'United Republic of Tanzania', 'Tanzania',\r",
				"    Country == 'United States of America', 'United States',\r",
				"    Country == 'Venezuela (Bolivarian Republic of)', 'Venezuela',\r",
				"    Country == 'Viet Nam', 'Vietnam',\r",
				"    Country\r",
				")) ~> countryNameFix4",
				"countryNameFix1 select(mapColumn(",
				"          Country,",
				"          Population,",
				"          {Pop. Density (per sq. mi.)},",
				"          {Infant mortality (per 1000 births)},",
				"          {GDP ($ per capita)},",
				"          {Literacy (%)},",
				"          Birthrate,",
				"          Deathrate",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"countryNameFix2 select(mapColumn(",
				"          Country,",
				"          {Happiness.Rank},",
				"          {Happiness.Score}",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select2",
				"countryNameFix3 select(mapColumn(",
				"          countries,",
				"          hf_score,",
				"          hf_rank",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select3",
				"countryNameFix4 select(mapColumn(",
				"          Country,",
				"          Status,",
				"          {Life expectancy },",
				"          {Income composition of resources},",
				"          Schooling",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select4",
				"select1, select2 join(select1@Country == select2@Country,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join1",
				"join1, select3 join(select1@Country == countries,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join2",
				"join2, select4 join(select1@Country == select4@Country,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join3",
				"join3 select(mapColumn(",
				"          Country = select1@Country,",
				"          Population,",
				"          {Pop. Density (per sq. mi.)},",
				"          {Infant mortality (per 1000 births)},",
				"          {GDP ($ per capita)},",
				"          {Literacy (%)},",
				"          Birthrate,",
				"          Deathrate,",
				"          {Happiness.Rank},",
				"          {Happiness.Score},",
				"          hf_score,",
				"          hf_rank,",
				"          Status,",
				"          {Life expectancy },",
				"          {Income composition of resources},",
				"          Schooling",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select5",
				"select5 derive({Pop. Density (per sq. mi.)} = replace({Pop. Density (per sq. mi.)}, ',', '.'),",
				"          {Infant mortality (per 1000 births)} = replace({Infant mortality (per 1000 births)}, ',', '.'),",
				"          {Literacy (%)} = replace({Literacy (%)}, ',', '.'),",
				"          Birthrate = replace(Birthrate, ',', '.'),",
				"          Deathrate = replace(Deathrate, ',', '.')) ~> derivedColumn1",
				"derivedColumn1 cast(output(",
				"          Country as string,",
				"          Population as integer,",
				"          {Pop. Density (per sq. mi.)} as double,",
				"          {Infant mortality (per 1000 births)} as double,",
				"          {GDP ($ per capita)} as double,",
				"          {Literacy (%)} as double,",
				"          Birthrate as double,",
				"          Deathrate as double,",
				"          {Happiness.Rank} as integer,",
				"          {Happiness.Score} as double,",
				"          hf_score as double,",
				"          hf_rank as integer,",
				"          Status as string,",
				"          {Life expectancy } as double,",
				"          {Income composition of resources} as double,",
				"          Schooling as double",
				"     ),",
				"     errors: true) ~> cast1",
				"cast1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          Country as string,",
				"          Population as string,",
				"          {Pop. Density (per sq. mi.)} as string,",
				"          {Infant mortality (per 1000 births)} as string,",
				"          {GDP ($ per capita)} as string,",
				"          {Literacy (%)} as string,",
				"          Birthrate as string,",
				"          Deathrate as string,",
				"          {Happiness.Rank} as string,",
				"          {Happiness.Score} as string,",
				"          hf_score as string,",
				"          hf_rank as string,",
				"          Status as string,",
				"          {Life expectancy } as string,",
				"          {Income composition of resources} as string,",
				"          Schooling as string",
				"     ),",
				"     partitionFileNames:['countriesoutput.csv'],",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     partitionBy('hash', 1)) ~> sink1"
			]
		}
	}
}